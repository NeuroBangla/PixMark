{
  "name": "pixmark",
  "version": "0.0.1",
  "description": "An annotation marking tool with React and TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "cosmos": "cosmos --expose-imports"
  },
  "keywords": [
    "annotation",
    "react",
    "typescript",
    "ts",
    "annotate"
  ],
  "author": "Maifee Ul Asad <maifeeulasad@gmail.com>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/maifeeulasad/pixmark.git"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.15.0",
    "prettier": "^3.4.1",
    "react-cosmos": "^6.2.1",
    "react-cosmos-dom": "^6.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}